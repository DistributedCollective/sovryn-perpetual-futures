{
  "name": "@sovryn/perpetual-swap",
  "version": "3.0.9",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "module.d.ts",
    "scripts/utils/*.ts",
    "scripts/utils/*.md",
    "dist/**/*.ts",
    "dist/**/*.js",
    "abi/**/*.json"
  ],
  "scripts": {
    "build": "npx hardhat compile && npx typechain --target ethers --outDir typechain 'artifacts/*.json'",
    "build:doc": "jsdoc2md --files ./scripts/utils/perpUtils.ts --configure ./jsdoc2md.json > ./scripts/utils/perpUtils.md && jsdoc2md --files ./scripts/utils/perpMath.ts --configure ./jsdoc2md.json > ./scripts/utils/perpMath.md && jsdoc2md --files ./scripts/utils/perpQueries.ts --configure ./jsdoc2md.json > ./scripts/utils/perpQueries.md && jsdoc2md --files ./scripts/utils/walletUtils.ts --configure ./jsdoc2md.json > ./scripts/utils/walletUtils.md",
    "size": "npx hardhat size-contracts",
    "generate": "npx typechain --target ethers --outDir typechain 'artifacts/contracts/**/*.json'",
    "prepublish": "npm run build:doc && tsc --outDir dist/ --project tsconfig.npm.json",
    "test": "npx hardhat test",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "deploy:network": "npx hardhat deploy --network",
    "deploy:localhost": "npm run-script deploy:network localhost",
    "deploy:bsc:testnet": "npm run-script deploy:network bscTestnet",
    "deploy:bsc:mainnet": "npm run-script deploy:network bscMainnet",
    "deploy:with-mocks:network": "cross-env-shell DEPLOY_MOCKS=true npx hardhat deploy --network",
    "deploy:with-mocks:bsc:testnet": "npm run-script deploy:with-mocks:network bscTestnet",
    "deploy:with-mocks:localhost": "npm run-script deploy:with-mocks:network localhost",
    "hard-redeploy:with-mocks:network": "cross-env-shell DEPLOY_MOCKS=true npx hardhat deploy --reset --network",
    "hard-redeploy:with-mocks:bsc:testnet": "npm run-script hard-redeploy:with-mocks:network bscTestnet",
    "hard-redeploy:with-mocks:localhost": "npm run-script hard-redeploy:with-mocks:network localhost",
    "hard-redeploy:without-pool:network": "cross-env-shell WITHOUT_POOL=true npx hardhat deploy --reset --network",
    "hard-redeploy:without-pool:bsc:testnet": "npm run-script hard-redeploy:without-pool:network bscTestnet",
    "hard-redeploy:without-pool:bsc:testnet:competition": "npm run-script hard-redeploy:without-pool:network bscTestnetCompetition",
    "hard-redeploy:without-pool:localhost": "npm run-script hard-redeploy:without-pool:network localhost",
    "hard-redeploy:network": "npx hardhat deploy --reset --network",
    "hard-redeploy:bsc:testnet": "npm run-script hard-redeploy:network bscTestnet",
    "hard-redeploy:without-pool:generic": "cross-env-shell WITHOUT_POOL=true npx hardhat deploy --reset"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@chainlink/contracts": "^0.4.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.5",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.0.8",
    "ethers-eip712": "^0.2.0",
    "hardhat": "^2.8.3",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.0",
    "hardhat-log-remover": "^2.0.2",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "mocha": "^8.2.1",
    "prettier": "^2.4.0",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.0.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@opengsn/provider": "^2.2.4",
    "@openzeppelin/contracts": "4.5.0",
    "@openzeppelin/contracts-upgradeable": "4.5.2",
    "@uniswap/lib": "^2.0.6",
    "@uniswap/v2-core": "^1.0.1",
    "bignumber.js": "^9.0.1",
    "browserify": "^17.0.0",
    "crypto-js": "^4.1.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.3",
    "hardhat-deploy": "^0.9.2",
    "mathjs": "^9.4.4",
    "newton-raphson-method": "^1.0.2",
    "node-fetch": "^2.6.6",
    "web3": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DistributedCollective/sovryn-perpetual-swap"
  }
}
